  "name": "Redar Browser",
  "icons": {
    "192": "android-chrome-192x192.png"
  },
  "background": {
    "scripts": ["static/js/background.js"]
  },
  "browser_action": {
    "default_icon": "android-chrome-192x192.png"
  },
  "permissions": [
    "http://*/",
    "https://*/"
  ]
}
